{
  "name": "mascotas_react_store",
  "version": "1.0.0",
  "description": "Modulo redux store para mascotas",
  "author": "nmarsollier",
  "license": "MIT",
  "repository": "nmarsollier/mascotas_react_store",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "mv node_modules/symbol-observable/es/index.js node_modules/symbol-observable/es/index.mjs & microbundle --no-compress --format modern,cjs",
    "start": "microbundle watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "tslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "16.13.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/node": "14.0.4",
    "@types/react": "16.9.35",
    "@types/react-redux": "7.1.9",
    "microbundle": "0.12.0",
    "babel-eslint": "10.0.3",
    "react-redux": "7.2.0",
    "npm-run-all": "4.1.5",
    "react-scripts": "3.4.1",
    "tslint": "6.1.2",
    "tslint-react": "5.0.0",
    "typescript": "3.9.3"
  },
  "files": [
    "dist"
  ]
}
